public class SharedDigit {
    public static boolean hasSharedDigit (int x, int y){
        boolean r= false;
        if ((x<10||x>99)||((y<10||y>99))){
            r=false ;
            
        }
        else {
               int a= x%10;
               int b= x/10;
               int c= y%10;
               int d= y/10;
               if(((a==c)||(a==d))||((b==c)||(b==d))){
                   r= true;
               }
            } return r;
        }
    }
